{{#section 'header'}}
		{{> iter}}
{{/section}}
    
    <section class="hero-wrap hero-wrap-2" style="background: #d11f41;">
      <div class="overlay"></div>
      <div class="container">
        <div class="row no-gutters slider-text align-items-center justify-content-center">
          <div class="col-md-9 ftco-animate text-center">
            <h1 class="mb-2 bread">Users Signed up</h1>
          </div>
        </div>
      </div>
    </section>

		
		<section class="ftco-section">
			<div class="container">
				<div class="row">
          <div class="col-lg-8 ftco-animate">         
            <div class="pt-5 mt-5">
              <h3 class="mb-5 h4 font-weight-bold">User Profiles</h3>
              <!-- START comment-list --> 
              <ul class="comment-list">
                {{#if accs}}
										{{#each accs}}
                      <li class="comment">
                        <div class="vcard bio">
                          <img src="images/teacher-1.jpg" alt="Image placeholder">
                        </div>
                        <div class="comment-body">
                          <h3>{{this.firstname}} {{this.lastname}}</h3>
                          <div class="meta mb-2">Phone: {{this.phone}}</div>
                          <p>{{this.message}}</p>
                          <p><a href="/accpay/{{@key}}" class="reply">view payment</a><a href="/viewchatsadminn" class="reply">view chats</a></p>
                        </div>
                      </li>
										{{/each}}
									{{else}}                            
                {{/if}}               
              </ul>
              <!-- END comment-list -->           
        </div>
			</div>
		</section>
{{#section 'footer'}}
	{{> foot}}
{{/section}}